    "bun-types": "^0.5.0",
    "eslint": "^8.33.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-unicorn": "^45.0.2",
    "prettier": "^2.8.3",
    "typescript": "^4.9.5",
    "vite": "^4.0.4"
  }
}
